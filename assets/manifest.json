{
    "manifest_version": 2,
    "name": "Undirect",
    "version": "2.0.0",
    "author": "Steve Leigh",
    "homepage_url": "https://github.com/xwipeoutx/undirect",
    "description": "Removes link tracking and redirection from facebook results",
    "icons": {
        "16": "undirect16.png",
        "48": "undirect48.png",
        "128": "undirect128.png"
    },
    "content_scripts": [{
        "matches": [
            "*://*.facebook.com/*"
        ],
        "js": ["undirect.js"]
    }]
}
